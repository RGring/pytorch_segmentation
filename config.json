{
    "arch": {
        "args": {
            "backbone": "resnet50",
            "freeze_backbone": false,
            "freeze_bn": false
        },
        "type": "PSPNet"
    },
    "ignore_index": 255,
    "loss": "CrossEntropyLoss2d",
    "lr_scheduler": {
        "args": {},
        "type": "Poly"
    },
    "n_gpu": 1,
    "name": "Rumex_PSPNet",
    "optimizer": {
        "args": {
            "lr": 0.01,
            "momentum": 0.9,
            "weight_decay": 0.0001
        },
        "differential_lr": true,
        "type": "SGD"
    },
    "train_loader": {
        "args": {
            "augment": true,
            "base_size": 816,
            "batch_size": 8,
            "blur": false,
            "crop_size": 816,
            "data_dir": "/home/rog/data/20200411_Odense",
            "flip": true,
            "num_workers": 8,
            "rotate": true,
            "scale": true,
            "shuffle": true,
            "split": "img_train"
        },
        "type": "Rumex"
    },
    "trainer": {
        "early_stop": 10,
        "epochs": 300,
        "log_dir": "saved/runs",
        "log_per_iter": 20,
        "monitor": "max Mean_IoU",
        "save_dir": "saved/",
        "save_period": 30,
        "tensorboard": true,
        "val": true,
        "val_per_epochs": 5
    },
    "use_synch_bn": true,
    "val_loader": {
        "args": {
            "batch_size": 8,
            "crop_size": 816,
            "data_dir": "/home/rog/data/20200411_Odense",
            "num_workers": 4,
            "split": "img_val",
            "val": true
        },
        "type": "Rumex"
    }
}